     "data": {
      "application/javascript": [
       "require.undef(\"op_art\");\n",
       "\n",
       "define(\"op_art\", [\"d3\"], function (d3) {\n",
       "  const COLOUR_SCALES = [\n",
       "    d3.interpolateBlues,\n",
       "    d3.interpolateOranges,\n",
       "    d3.interpolatePurples,\n",
       "    d3.interpolateGreens,\n",
       "  ];\n",
       "\n",
       "  function format(v) {\n",
       "    if (typeof v == \"boolean\") {\n",
       "      return v ? \"True\" : \"False\";\n",
       "    } else if (typeof v == \"number\") {\n",
       "      if (Number.isInteger(v)) {\n",
       "        return v;\n",
       "      }\n",
       "      return d3.format(\",.2g\")(v);\n",
       "    }\n",
       "    return v;\n",
       "  }\n",
       "\n",
       "  function shape_2d_repr(arr) {\n",
       "    const s = arr.shape;\n",
       "    if (s.length == 0) {\n",
       "      return [1, 1];\n",
       "    } else if (s.length == 1) {\n",
       "      return [s[0], 1];\n",
       "    } else if (s.length == 2) {\n",
       "      return [s[1], s[0]];\n",
       "    } else if (s.length == 3) {\n",
       "      // TODO: why '- 1' below?\n",
       "      const stacking = (s[0] - 1) * 0.5;\n",
       "      return [s[2] + stacking, s[1] + stacking];\n",
       "    }\n",
       "  }\n",
       "\n",
       "  function index_to_x(index) {\n",
       "    if (index.length == 0) {\n",
       "      return 0;\n",
       "    } else if (index.length == 1) {\n",
       "      return index[0];\n",
       "    } else if (index.length == 2) {\n",
       "      return index[1];\n",
       "    } else if (index.length == 3) {\n",
       "      const stacking = index[0] * 0.5;\n",
       "      return index[2] + stacking;\n",
       "    }\n",
       "  }\n",
       "\n",
       "  function index_to_y(index, shape) {\n",
       "    if (index.length == 0) {\n",
       "      return 0;\n",
       "    } else if (index.length == 1) {\n",
       "      return 0;\n",
       "    } else if (index.length == 2) {\n",
       "      return index[0];\n",
       "    } else if (index.length == 3) {\n",
       "      const stacking = (shape[0] - index[0]) * 0.5;\n",
       "      return index[1] + stacking;\n",
       "    }\n",
       "  }\n",
       "\n",
       "  function plot_array_internal_offset(\n",
       "    top_g,\n",
       "    g,\n",
       "    ndarr,\n",
       "    cellWidth,\n",
       "    cellHeight,\n",
       "    index,\n",
       "    xOffset,\n",
       "    idToYoffset,\n",
       "    idToRepr,\n",
       "    dataExtent,\n",
       "    duration\n",
       "  ) {\n",
       "    const x = d3\n",
       "      .scaleLinear()\n",
       "      .domain([0, 10])\n",
       "      .range([0, 10 * cellWidth]);\n",
       "    const y = d3\n",
       "      .scaleLinear()\n",
       "      .domain([0, 10])\n",
       "      .range([0, 10 * cellHeight]);\n",
       "\n",
       "    const yOffset = idToYoffset[ndarr.id];\n",
       "\n",
       "    // Draw dimension lines\n",
       "    if (ndarr.ndim == 1) {\n",
       "      const ind1 = [0];\n",
       "      const ind2 = [ndarr.shape[0]];\n",
       "      g.append(\"line\")\n",
       "        .attr(\"class\", \"dimension_line\")\n",
       "        .attr(\"x1\", x(index_to_x(ind1)) + xOffset)\n",
       "        .attr(\"y1\", y(index_to_y(ind1, ndarr.shape) + 1) + yOffset)\n",
       "        .attr(\"x2\", x(index_to_x(ind2) - 0.1) + xOffset)\n",
       "        .attr(\"y2\", y(index_to_y(ind2, ndarr.shape) + 1) + yOffset);\n",
       "    } else if (ndarr.ndim == 2) {\n",
       "      const ind1 = [0, 0];\n",
       "      const ind2 = [ndarr.shape[0], 0];\n",
       "      const ind3 = [ndarr.shape[0], ndarr.shape[1]];\n",
       "      g.append(\"line\")\n",
       "        .attr(\"class\", \"dimension_line\")\n",
       "        .attr(\"x1\", x(index_to_x(ind1) - 0.1) + xOffset)\n",
       "        .attr(\"y1\", y(index_to_y(ind1, ndarr.shape)) + yOffset)\n",
       "        .attr(\"x2\", x(index_to_x(ind2) - 0.1) + xOffset)\n",
       "        .attr(\"y2\", y(index_to_y(ind2, ndarr.shape)) + yOffset);\n",
       "      g.append(\"line\")\n",
       "        .attr(\"class\", \"dimension_line\")\n",
       "        .attr(\"x1\", x(index_to_x(ind2) - 0.1) + xOffset)\n",
       "        .attr(\"y1\", y(index_to_y(ind2, ndarr.shape)) + yOffset)\n",
       "        .attr(\"x2\", x(index_to_x(ind3) - 0.1) + xOffset)\n",
       "        .attr(\"y2\", y(index_to_y(ind3, ndarr.shape)) + yOffset);\n",
       "    } else if (ndarr.ndim == 3) {\n",
       "      const ind1 = [ndarr.shape[0], 0, 0];\n",
       "      const ind2 = [0, 0, 0];\n",
       "      const ind3 = [0, ndarr.shape[1], 0];\n",
       "      const ind4 = [0, ndarr.shape[1], ndarr.shape[2]];\n",
       "      g.append(\"line\")\n",
       "        .attr(\"class\", \"dimension_line\")\n",
       "        .attr(\"x1\", x(index_to_x(ind1) - 0.1) + xOffset)\n",
       "        .attr(\"y1\", y(index_to_y(ind1, ndarr.shape)) + yOffset)\n",
       "        .attr(\"x2\", x(index_to_x(ind2) - 0.1) + xOffset)\n",
       "        .attr(\"y2\", y(index_to_y(ind2, ndarr.shape)) + yOffset);\n",
       "      g.append(\"line\")\n",
       "        .attr(\"class\", \"dimension_line\")\n",
       "        .attr(\"x1\", x(index_to_x(ind2) - 0.1) + xOffset)\n",
       "        .attr(\"y1\", y(index_to_y(ind2, ndarr.shape)) + yOffset)\n",
       "        .attr(\"x2\", x(index_to_x(ind3) - 0.1) + xOffset)\n",
       "        .attr(\"y2\", y(index_to_y(ind3, ndarr.shape)) + yOffset);\n",
       "      g.append(\"line\")\n",
       "        .attr(\"class\", \"dimension_line\")\n",
       "        .attr(\"x1\", x(index_to_x(ind3) - 0.1) + xOffset)\n",
       "        .attr(\"y1\", y(index_to_y(ind3, ndarr.shape)) + yOffset)\n",
       "        .attr(\"x2\", x(index_to_x(ind4) - 0.1) + xOffset)\n",
       "        .attr(\"y2\", y(index_to_y(ind4, ndarr.shape)) + yOffset);\n",
       "    }\n",
       "\n",
       "    plot_cells_internal(\n",
       "      top_g,\n",
       "      g,\n",
       "      ndarr.cells,\n",
       "      cellWidth,\n",
       "      cellHeight,\n",
       "      index,\n",
       "      xOffset,\n",
       "      idToYoffset,\n",
       "      idToRepr,\n",
       "      dataExtent,\n",
       "      duration,\n",
       "      0\n",
       "    );\n",
       "  }\n",
       "\n",
       "  function plot_cells_internal(\n",
       "    top_g,\n",
       "    g,\n",
       "    cells,\n",
       "    cellWidth,\n",
       "    cellHeight,\n",
       "    index,\n",
       "    xOffset,\n",
       "    idToYoffset,\n",
       "    idToRepr,\n",
       "    dataExtent,\n",
       "    duration,\n",
       "    delay,\n",
       "    callback\n",
       "  ) {\n",
       "    const x = d3\n",
       "      .scaleLinear()\n",
       "      .domain([0, 10])\n",
       "      .range([0, 10 * cellWidth]);\n",
       "    const y = d3\n",
       "      .scaleLinear()\n",
       "      .domain([0, 10])\n",
       "      .range([0, 10 * cellHeight]);\n",
       "\n",
       "    const colour = d3\n",
       "      .scaleSequential()\n",
       "      .domain([dataExtent[0], dataExtent[1] * 1.75]) // don't use full upper range\n",
       "      .interpolator(COLOUR_SCALES[index % COLOUR_SCALES.length]);\n",
       "\n",
       "    function frontToBack(a, b) {\n",
       "      return b.index[0] - a.index[0];\n",
       "    }\n",
       "\n",
       "    const t = g.transition().delay(delay).duration(duration);\n",
       "\n",
       "    const cellElts = g\n",
       "      .selectAll(\".cell\")\n",
       "      .data(cells, (d) => d.key)\n",
       "      .join(\n",
       "        (enter) => {\n",
       "          const gCell = enter\n",
       "            .append(\"g\")\n",
       "            .attr(\n",
       "              \"transform\",\n",
       "              (d) =>\n",
       "                `translate(${x(index_to_x(d.index)) + xOffset}, ${\n",
       "                  y(index_to_y(d.index, idToRepr[d.id.split(\"_\")[0]].shape)) +\n",
       "                  idToYoffset[d.id.split(\"_\")[0]]\n",
       "                })`\n",
       "            )\n",
       "            .attr(\"visibility\", \"hidden\");\n",
       "          gCell\n",
       "            .append(\"rect\")\n",
       "            .attr(\"class\", \"cell-rect\")\n",
       "            .attr(\"x\", 0)\n",
       "            .attr(\"y\", 0)\n",
       "            .attr(\"width\", x(0.9) - x(0))\n",
       "            .attr(\"height\", y(0.9) - y(0))\n",
       "            .attr(\"fill\", (d) => colour(d.value));\n",
       "          gCell\n",
       "            .append(\"text\")\n",
       "            .attr(\"class\", \"cell-text\")\n",
       "            .text((d) => d.value)\n",
       "            .attr(\"x\", 0)\n",
       "            .attr(\"y\", 0)\n",
       "            .attr(\"dx\", x(0.45))\n",
       "            .attr(\"dy\", y(0.45));\n",
       "          gCell\n",
       "            .transition()\n",
       "            .duration(0)\n",
       "            .delay(delay)\n",
       "            .attr(\"visibility\", \"visible\");\n",
       "          return gCell;\n",
       "        },\n",
       "        (update) =>\n",
       "          update.call((update) =>\n",
       "            update\n",
       "              .transition(t)\n",
       "              .attr(\n",
       "                \"transform\",\n",
       "                (d) =>\n",
       "                  `translate(${x(index_to_x(d.index)) + xOffset}, ${\n",
       "                    y(index_to_y(d.index, idToRepr[d.id.split(\"_\")[0]].shape)) +\n",
       "                    idToYoffset[d.id.split(\"_\")[0]]\n",
       "                  })`\n",
       "              )\n",
       "              .end()\n",
       "              .then(() => {\n",
       "                console.log(\"transition ended\");\n",
       "                update.remove();\n",
       "                callback();\n",
       "              })\n",
       "          ),\n",
       "        (exit) =>\n",
       "          exit.call((exit) => exit.transition(t).attr(\"fill\", \"white\").remove()) // fade out\n",
       "      )\n",
       "      .attr(\"class\", \"cell\");\n",
       "    cellElts.sort(frontToBack);\n",
       "  }\n",
       "\n",
       "  function computeTextWidths(container, textData) {\n",
       "    // Measure the display widths of each string in textData\n",
       "    // From https://stackoverflow.com/a/37528489\n",
       "    const textWidths = [];\n",
       "\n",
       "    const svgTemp = d3\n",
       "      .select(container)\n",
       "      .append(\"svg\")\n",
       "      .attr(\"width\", 100)\n",
       "      .attr(\"height\", 40);\n",
       "\n",
       "    svgTemp\n",
       "      .append(\"g\")\n",
       "      .selectAll(\".dummyText\") // declare a new CSS class 'dummyText'\n",
       "      .data(textData)\n",
       "      .enter() // create new element\n",
       "      .append(\"text\") // add element to class\n",
       "      .attr(\"class\", \"fixed-text\")\n",
       "      .attr(\"opacity\", 0.0) // so it's invisible\n",
       "      .text((d) => format(d))\n",
       "      .each(function (d, i) {\n",
       "        const thisWidth = this.getComputedTextLength();\n",
       "        textWidths.push(thisWidth);\n",
       "        this.remove(); // remove them just after displaying them\n",
       "      });\n",
       "\n",
       "    svgTemp.remove();\n",
       "\n",
       "    return textWidths;\n",
       "  }\n",
       "\n",
       "  function computeCellWidth(container, objects, minCellWidth) {\n",
       "    const valuesAsText = [];\n",
       "    for (const o of objects) {\n",
       "      if (!isString(o)) {\n",
       "        const vals = o.cells.map((d) => format(d.value));\n",
       "        valuesAsText.push(...vals);\n",
       "      }\n",
       "    }\n",
       "    const max = Math.max(...computeTextWidths(container, valuesAsText));\n",
       "    return Math.max(minCellWidth, max);\n",
       "  }\n",
       "\n",
       "  function computeArrayWidths(arrays, cellWidth) {\n",
       "    return arrays.map((array) => shape_2d_repr(array)[0] * cellWidth);\n",
       "  }\n",
       "\n",
       "  function computeArrayHeights(arrays, cellHeight) {\n",
       "    return arrays.map((array) => shape_2d_repr(array)[1] * cellHeight);\n",
       "  }\n",
       "\n",
       "  function isString(s) {\n",
       "    return typeof s === \"string\" || s instanceof String;\n",
       "  }\n",
       "\n",
       "  function computeWidths(container, objects, cellWidth) {\n",
       "    return objects.map((o) => {\n",
       "      if (isString(o)) {\n",
       "        return computeTextWidths(container, [o])[0];\n",
       "      } else {\n",
       "        return computeArrayWidths([o], cellWidth)[0];\n",
       "      }\n",
       "    });\n",
       "  }\n",
       "\n",
       "  function computeHeights(container, objects, cellHeight) {\n",
       "    return objects.map((o) => {\n",
       "      if (isString(o)) {\n",
       "        return 0;\n",
       "      } else if (!isString(o)) {\n",
       "        return computeArrayHeights([o], cellHeight)[0];\n",
       "      }\n",
       "    });\n",
       "  }\n",
       "\n",
       "  function noSources(representation) {\n",
       "    return representation.cells.every((cell) => !cell.sources);\n",
       "  }\n",
       "\n",
       "  function animatePerCell(representation) {\n",
       "    // Animate each cell individually only if the output array\n",
       "    // depends on a large number of inputs. We define that as\n",
       "    // a cell having more than one source array and more than one cell\n",
       "    // from a source array.\n",
       "\n",
       "    if (representation.cells.length == 0) {\n",
       "      return false;\n",
       "    }\n",
       "\n",
       "    const cell = representation.cells[0]; // use first cell\n",
       "    const sourceArrays = cell.sources.map((id) => id.split(/_/)[0]);\n",
       "    const sourceCounts = sourceArrays.reduce(\n",
       "      (acc, e) => acc.set(e, (acc.get(e) || 0) + 1),\n",
       "      new Map()\n",
       "    );\n",
       "    const numSourceArrays = [...sourceCounts.keys()].length;\n",
       "    const maxCellsInSourceArray = Math.max(...sourceCounts.values());\n",
       "    if (numSourceArrays == 1 || maxCellsInSourceArray == 1) {\n",
       "      return false;\n",
       "    }\n",
       "    return true;\n",
       "  }\n",
       "\n",
       "  // objects is an array of representations\n",
       "  function visualize(container, objects, lines, duration) {\n",
       "    const reprs = objects.map((o) => o.representation);\n",
       "    const cellHeight = 30;\n",
       "    const cellPadding = 20;\n",
       "    const minCellWidth = cellHeight;\n",
       "    const cellWidth = computeCellWidth(container, reprs, minCellWidth);\n",
       "    const objectWidths = computeWidths(container, reprs, cellWidth);\n",
       "\n",
       "    const objectHeights = computeHeights(container, reprs, cellHeight).map(\n",
       "      (d) => d + cellPadding\n",
       "    );\n",
       "\n",
       "    const codeWidths = computeTextWidths(container, lines);\n",
       "    const maxCodeWidth = Math.max(...codeWidths) + cellPadding;\n",
       "\n",
       "    const idToIndex = {};\n",
       "    const idToYoffset = {};\n",
       "    const idToRepr = {};\n",
       "    let cumulativeHeight = 0;\n",
       "    for (let i = 0; i < objectHeights.length; i++) {\n",
       "      idToIndex[reprs[i].id] = i;\n",
       "      idToYoffset[reprs[i].id] = cumulativeHeight;\n",
       "      cumulativeHeight += objectHeights[i];\n",
       "      idToRepr[reprs[i].id] = reprs[i];\n",
       "    }\n",
       "    const width = Math.max(...objectWidths) + maxCodeWidth;\n",
       "    const height = objectHeights.reduce((sum, current) => sum + current, 0);\n",
       "\n",
       "    const allCells = objects.map((o) => o.representation.cells).flat(1);\n",
       "    const dataExtent = d3.extent(allCells, (d) => d.value);\n",
       "\n",
       "    // append the svg object to the body of the page\n",
       "    const svg = d3\n",
       "      .select(container)\n",
       "      .append(\"svg\")\n",
       "      .attr(\"width\", width)\n",
       "      .attr(\"height\", height);\n",
       "\n",
       "    const g = svg.append(\"g\");\n",
       "\n",
       "    function just_show(arr, line) {\n",
       "      // TODO: don't need to show the code line twice\n",
       "      g.append(\"g\")\n",
       "        .attr(\n",
       "          \"transform\",\n",
       "          \"translate(0, \" + idToYoffset[arr.representation.id] + \")\"\n",
       "        )\n",
       "        .append(\"text\")\n",
       "        .attr(\"class\", \"fixed-text\")\n",
       "        .text(line)\n",
       "        .attr(\"x\", 0)\n",
       "        .attr(\"y\", 0);\n",
       "      let xOffset = maxCodeWidth;\n",
       "      const group = g.append(\"g\");\n",
       "      const index = idToIndex[arr.representation.id];\n",
       "      plot_array_internal_offset(\n",
       "        g,\n",
       "        group,\n",
       "        arr.representation,\n",
       "        cellWidth,\n",
       "        cellHeight,\n",
       "        0, // don't use index\n",
       "        xOffset,\n",
       "        idToYoffset,\n",
       "        idToRepr,\n",
       "        dataExtent,\n",
       "        duration\n",
       "      );\n",
       "    }\n",
       "\n",
       "    // make a dictionary of all the cells, keyed by ID\n",
       "    const cellIdToCell = {};\n",
       "    for (const o of objects) {\n",
       "      for (const cell of o.representation.cells) {\n",
       "        cellIdToCell[cell.id] = cell;\n",
       "      }\n",
       "    }\n",
       "\n",
       "    function animate(objects, lines, index) {\n",
       "      // animate every cell in objects[index] from its inputs\n",
       "      const object = objects[index];\n",
       "\n",
       "      // show line\n",
       "      const line = lines[index];\n",
       "      g.append(\"g\")\n",
       "        .attr(\"transform\", \"translate(0, \" + idToYoffset[object.id] + \")\")\n",
       "        .append(\"text\")\n",
       "        .attr(\"class\", \"fixed-text\")\n",
       "        .text(line)\n",
       "        .attr(\"x\", 0)\n",
       "        .attr(\"y\", 0);\n",
       "\n",
       "      // show object\n",
       "      just_show(object, line);\n",
       "\n",
       "      // If no animation go to next\n",
       "      if (noSources(object.representation)) {\n",
       "        if (index < objects.length - 1) {\n",
       "          animate(objects, lines, index + 1);\n",
       "        }\n",
       "        return;\n",
       "      }\n",
       "\n",
       "      // Dim other cells during transition\n",
       "      g.selectAll(\".cell\").attr(\"fill-opacity\", 0.05);\n",
       "\n",
       "      // Divide cells into groups that are animated together, using heuristic\n",
       "      // in animatePerCell to decide if to animate whole array at once - or per-cell\n",
       "      const cellGroups = animatePerCell(object.representation)\n",
       "        ? object.representation.cells.map((cell) => [cell])\n",
       "        : [object.representation.cells];\n",
       "\n",
       "      // Each group has a separate delay\n",
       "      let delay = 0;\n",
       "\n",
       "      for (let i = 0; i < cellGroups.length; i++) {\n",
       "        const cellGroup = cellGroups[i];\n",
       "        const startCells = [];\n",
       "        const endCells = [];\n",
       "        for (const cell of cellGroup) {\n",
       "          if (cell.sources) {\n",
       "            for (const inputCellId of cell.sources) {\n",
       "              const startCell = Object.assign({}, cellIdToCell[inputCellId]);\n",
       "              const endCell = Object.assign({}, cell);\n",
       "              // create a common key for object constancy\n",
       "              const commonKey = startCell.id + \"_\" + endCell.id;\n",
       "              startCell.key = commonKey;\n",
       "              endCell.key = commonKey;\n",
       "              startCells.push(startCell);\n",
       "              endCells.push(endCell);\n",
       "            }\n",
       "          }\n",
       "        }\n",
       "\n",
       "        // Now show startCells, and transition to endCells\n",
       "\n",
       "        let xOffset = maxCodeWidth;\n",
       "        const group = g.append(\"g\");\n",
       "\n",
       "        plot_cells_internal(\n",
       "          g,\n",
       "          group,\n",
       "          startCells,\n",
       "          cellWidth,\n",
       "          cellHeight,\n",
       "          0,\n",
       "          xOffset,\n",
       "          idToYoffset,\n",
       "          idToRepr,\n",
       "          dataExtent,\n",
       "          duration,\n",
       "          delay\n",
       "        );\n",
       "\n",
       "        const afterCellGroup = () => {\n",
       "          g.selectAll(\".cell\")\n",
       "            .filter((d) => cellGroup.map((cell) => cell.id).includes(d.id))\n",
       "            .attr(\"fill-opacity\", 1);\n",
       "        };\n",
       "\n",
       "        const afterLastCellGroup = () => {\n",
       "          // Undim everything\n",
       "          g.selectAll(\".cell\").attr(\"fill-opacity\", 1);\n",
       "          if (index < objects.length - 1) {\n",
       "            animate(objects, lines, index + 1);\n",
       "          } else {\n",
       "            addMouseovers();\n",
       "            showReloadButton();\n",
       "          }\n",
       "        };\n",
       "\n",
       "        plot_cells_internal(\n",
       "          g,\n",
       "          group,\n",
